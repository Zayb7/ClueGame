package clueGame;

import java.awt.Dimension;

import javax.swing.JFrame;

public class ControlGUI extends JFrame {

	public ControlGUI(){
		setSize(new Dimension(1000, 300));
	}
	
	public static void main(String args[]){
		ControlGUI gui = new ControlGUI();
		gui.setVisible(true);
	}
}
